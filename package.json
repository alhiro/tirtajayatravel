{
  "name": "tirtajaya",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run write:env -s && ng build",
    "deploy": "npm run write:env -s && ng build && firebase login:ci && firebase deploy --only hosting",
    "start": "npm run write:env -s && ng serve --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\"",
    "test": "npm run write:env -s && ng test",
    "test:ci": "npm run write:env -s && npm run lint -s && ng run tirtajaya:test:ci",
    "e2e": "npm run write:env -s && ng e2e",
    "cypress:open": "npm run write:env -s && ng run tirtajaya:cypress-open",
    "cypress:run": "npm run write:env -s && ng run tirtajaya:cypress-run",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "docs": "hads ./docs -o",
    "compodoc": "compodoc -p tsconfig.json -d docs/generated -s -o",
    "write:env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s && husky install",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "~18.0.5",
    "@angular/cdk": "^18.0.6",
    "@angular/common": "~18.0.5",
    "@angular/compiler": "~18.0.5",
    "@angular/core": "~18.0.5",
    "@angular/forms": "~18.0.5",
    "@angular/localize": "~18.0.5",
    "@angular/material": "^18.0.6",
    "@angular/platform-browser": "~18.0.5",
    "@angular/platform-browser-dynamic": "~18.0.5",
    "@angular/router": "~18.0.5",
    "@angular/service-worker": "~18.0.5",
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@popperjs/core": "^2.11.0",
    "angulartics2": "^12.1.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "html2canvas": "^1.4.1",
    "jquery": "^3.7.1",
    "jspdf": "^2.5.2",
    "jwt-decode": "^4.0.0",
    "line-awesome": "^1.3.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.4",
    "ng-apexcharts": "^1.11.0",
    "ng-inline-svg-2": "^15.0.1",
    "ngx-easy-table": "^15.6.0",
    "node-sass": "^9.0.0",
    "object-path": "^0.11.8",
    "prism-themes": "^1.9.0",
    "rxjs": "^7.5.0",
    "sass": "^1.77.4",
    "socicon": "^3.0.5",
    "sweetalert2": "^11.12.4",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "zone.js": "^0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.0.6",
    "@angular-eslint/builder": "~14.0.3",
    "@angular-eslint/eslint-plugin": "~14.0.3",
    "@angular-eslint/eslint-plugin-template": "~14.0.3",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "~14.0.3",
    "@angular/cli": "~18.0.6",
    "@angular/compiler-cli": "~18.0.5",
    "@angular/language-service": "~18.0.5",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@cypress/schematic": "^2.0.3",
    "@ngneat/until-destroy": "^9.0.0",
    "@ngx-rocket/scripts": "^5.2.2",
    "@types/jasmine": "^4.0.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/jquery": "^3.5.30",
    "@types/lodash": "^4.14.184",
    "@types/node": "^14.18.26",
    "@types/object-path": "^0.11.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "~5.34.0",
    "@typescript-eslint/parser": "~5.34.0",
    "cypress": "~10.6.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "hads": "^3.0.0",
    "https-proxy-agent": "^5.0.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "postcss": "^8.4.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint": "~14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "~7.0.0",
    "stylelint-config-standard": "~28.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^5.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
